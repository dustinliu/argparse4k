<?xml version="1.0" encoding="UTF-8"?>
<configuration>

    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>[%thread] %F:%L - %msg%n</pattern>
        </encoder>
    </appender>

    <logger name="org.dustinl.argparse4k" level="TRACE"/>

    <root level="ERROR">
        <appender-ref ref="STDOUT" />
    </root>

</configuration>